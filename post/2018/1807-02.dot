    t2_req_msg_3 [label="Request 3"];
    t2_req_msg_4 [label="Request 4"];
    t2_act_msg_1 [label="Action 1"];
    t2_act_msg_2 [label="Action 2"];
    t2_act_msg_3 [label="Action 3"];
    t2_act_msg_4 [label="Action 4"];
